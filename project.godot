config_version=4

[application]

config/name="Kiawa"
run/main_scene="res://Scenes/Screens/Main.tscn"
config/icon="res://icon.png"

[autoload]

GlobalProcess="*res://Scenes/GlobalProcess.tscn"
ResourceDirector="*res://Scenes/ResourceDirector.tscn"
Preferences="*res://Scenes/Data/Preferences.tscn"
GlobalAudioStream="*res://Scenes/Audio/GlobalAudioStream.tscn"
MenuAudioStream="*res://Scenes/Audio/MenuAudioStream.tscn"
Firebase="*res://Scenes/Data/Firebase.tscn"

[display]

window/size/width=1280
window/size/height=720
window/size/resizable=false
window/vsync/use_vsync=false
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray(  )

[gd2cs]

config/ask_no_python=true
config/python="py"

[global]

"auto load"=false

[gui]

common/drop_mouse_on_gui_input_disabled=true

